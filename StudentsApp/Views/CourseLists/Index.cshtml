@model IEnumerable<StudentsApp.Models.CourseList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Durtation)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Course.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Durtation)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
